{
  "name": "@galacean/engine-shaderlab",
  "version": "1.5.10",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "debug": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./verbose": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "verbose": [
        "./src/index.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "main": "dist/main.js",
    "module": "dist/module.js",
    "types": "types/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/module.js",
        "require": "./dist/main.js"
      },
      "./verbose": {
        "import": "./dist/module.verbose.js",
        "require": "./dist/module.verbose.js"
      }
    },
    "typesVersions": {
      "*": {
        "parser": [
          "./types/index.d.ts"
        ]
      }
    }
  },
  "repository": {
    "url": "https://github.com/galacean/engine.git"
  },
  "license": "MIT",
  "scripts": {
    "b:types": "tsc"
  },
  "umd": {
    "name": "Galacean.ShaderLab",
    "globals": {
      "@galacean/engine": "Galacean"
    }
  },
  "files": [
    "dist/**/*",
    "types/**/*",
    "verbose/package.json"
  ],
  "devDependencies": {
    "@galacean/engine-design": "workspace:*",
    "@galacean/engine": "workspace:*"
  },
  "peerDependencies": {
    "@galacean/engine": "workspace:*"
  }
}
